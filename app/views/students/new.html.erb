<h3> Students Form </h3>

<form action="<%= students_path %>" method="POST">
  <label>Student name:</label><br>
  <input type="text" id="student_name" name="student[name]"><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
